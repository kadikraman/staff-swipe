{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "rules": {
    "react/jsx-filename-extension": [1, { "extensions": [".js"] }],
    "no-underscore-dangle": 0,
    "arrow-parens": ["error", "as-needed"]
  },
  "globals": {
    "describe": false,
    "document": false,
    "expect": false,
    "it": false,
  }
}
